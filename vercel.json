{
    "version": 2,
    "builds": [
        {
            "src": "API_CRUD_REST/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "API_CRUD_REST/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE"]
        },
        {
            "src": "/(.*)",
            "dest": "API_CRUD_REST/$1"
        }
    ]
}
